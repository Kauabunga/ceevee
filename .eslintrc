{
  "extends": ["airbnb", "prettier"],
  "globals": {
    "__DEV__": true
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": 2,
    "react/function-component-definition": [
      2,
      { "namedComponents": "function-declaration" }
    ],
    "no-unused-vars": 1,
    "react/no-danger": 0,
    "react/prop-types": 0,
    "react/jsx-props-no-spreading": 0,
    "react/jsx-filename-extension": 0,
    "import/prefer-default-export": 0
  }
}
