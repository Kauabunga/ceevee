{
  "extends": ["airbnb", "prettier"],
  "globals": {
    "__DEV__": true
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": 2,
    "no-unused-vars": 1,
    "react/no-danger": 0,
    "react/prop-types": 0,
    "react/jsx-props-no-spreading": 0,
    "react/jsx-filename-extension": 0
  }
}
